package thirdSession;

import java.util.Arrays;

public class PleaseIgnore {
	public static void main(String[] args) {
		String []as={"v","s","r","y","j","m","a","c","x","z","y","e","j","i","o"};
		
		Arrays.sort(as);
		for (int i = 0; i < as.length; i++) {
			System.out.println(as[i]+"\t"+i);
			
		}
		String key="j";
		
		String j="j";
		
		String a="a";
		
		int index=Arrays.binarySearch(as, key);
		
		System.out.println(index);
		
		System.out.println(key+" "+j);
		
	}
}
