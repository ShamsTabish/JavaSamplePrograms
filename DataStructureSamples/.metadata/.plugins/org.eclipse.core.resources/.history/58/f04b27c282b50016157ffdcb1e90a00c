
public class InheritanceAndCompositionDemo {

}

class Vehicle{
  	 Engine engine;//Vehicle is composed of Engine
  	 Vehicle(String engineType){
  		 
  	 }
}

class Engine{
	String type;
	Engine(String type){
		this.type=type;
	}
}


class Truck extends Vehicle{ // Inheritance,  Truck is a vehicle
	
}