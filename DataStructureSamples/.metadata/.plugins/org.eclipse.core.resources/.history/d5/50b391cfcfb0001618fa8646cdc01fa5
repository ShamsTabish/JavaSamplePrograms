package branching;

public class SwitchCaseDemo {
	public static void main(String[] args) {
		String today="Mondays";
		
		
		System.out.println("Today is "+today+" and I'll study ");
		switch(today){
		case "Monday":
			System.out.println("Java");
			break;
		case "Wednesday":
			System.out.println("Android");
			break;
		case "Thursday":
			System.out.println("Big Data");
			break;
		default:
			System.out.println("----");
		};
	}
}
