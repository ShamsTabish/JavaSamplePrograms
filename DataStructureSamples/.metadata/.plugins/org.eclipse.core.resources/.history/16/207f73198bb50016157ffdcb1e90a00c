
public class OverridingDemo {
	public static void main(String[] args) {
		MyVehicle petVicle=new MyCar();
		petVicle.drive();
		
	}
}

class MyVehicle{
	void drive(){
		System.out.println("Driving the vehicle");
	}
}

class MyCar extends MyVehicle{
	void drive(){
		System.out.println("Driving my Super Carr...");
	}
	
}
