package fifthSession;

public class AbstractClassDemo {
	public static void main(String[] args) {

	}
}

abstract class Vehicle{
	String engine;
	abstract void move();
	void startEngine(){
		System.out.println("Just use the key to start the Engine..");
	}
}

class Car extends Vehicle{
	void move(){
		startEngine();
		System.out.println();
	}
}